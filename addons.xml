<addons>

<addon id="plugin.video.prowiz" name="ProStreams ToolKit" version="0.0.2" provider-name="any">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">ProStreams ToolKit</summary>
    <description lang="en">Here to make life easier.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.prolive"
       name="Pro Live"
       version="0.0.3"
       provider-name="anybody">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Pro Live Streams</summary>
    <language>en</language>
    <description lang="en">Pro Live Addon</description>
     <platform>all</platform>
  </extension>
</addon>

<addon id="script.proskins" name="Pro Skins" version="0.0.1" provider-name="Prostreams">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Pro Stream TV Guide Skins</summary>
    <description lang="en">TV Guide Skins Thanks to all authors of the code</description>
    <platform>all</platform>
  </extension>
</addon>


<addon id="plugin.video.stalker" name="IPTV Stalker Papiao" version="10.0.0c" provider-name="dara">
    <requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.service" library="server.py" start="startup" />
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">IPTV Stalker Papiao</summary>
		<description lang="en">IPTV Stalker for Papiao servers, based on original code by esxbr </description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<forum></forum>
	</extension>
</addon>

<addon id="repo.prostreams" name="ProStreams Repository" version="0.0.1" provider-name="ProStreams">
	<extension point="xbmc.addon.repository" name="Pro streams TV Guide Repository">
		<info compressed="false">https://raw.githubusercontent.com/prostreams/repo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/prostreams/repo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/prostreams/repo/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install ProStreams Dedicated TV Guide </summary>
		<description>Download and install Routine for prostreams addons </description>
		<disclaimer> Repository for the Dedicated ProStreams Tv guide addon </disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.prostreams" name="ProStreams TV Guide" version="0.0.7" provider-name="ProStreams">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.simplejson" version="2.0.10"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py"/>
    <extension point="xbmc.service" library="service.py" start="login"/>
	<extension point="xbmc.addon.metadata">
	<provides>video</provides>
	<provides>programs</provides>
		<description lang="en">ProStreams TV Guide dedicated tv guide for iptv Stalker systems</description>
		<disclaimer lang="en">Prostreams Guide is forked and based on FTV Guide, ontapp , Ivue , tommy winthers original code, </disclaimer>
	</extension>
</addon>

</addons>
